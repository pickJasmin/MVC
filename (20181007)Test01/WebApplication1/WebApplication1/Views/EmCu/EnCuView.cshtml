@*引用命名空间*@
@using WebApplication1.ViewModels;
@* 创建View的强类型 *@
@model AddViewModel
@{
    Layout = null;
    //强制类型转换
    //Employee emp = (Employee)ViewData["EmpKey"];
    //Employee emp = ViewBag.EmpKey;

}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>MyView</title>
</head>
<body>
    @*<h1>@emp.Name,@((string)ViewData["greeting"])!</h1>*@
    <h1>@Model.EmployeeName,@ViewBag.Greeting</h1>

    <div>
        <h2>员工信息</h2>
        @*获取数据值*@
        <p>姓名：@Model.EmployeeName</p>
        <p>工资：@Model.EmployeeSalary</p>
        <p>级别：@Model.EmployeeGrade</p>
    </div>

    <h1>@Model.CustomerName,@ViewBag.Greeting </h1>
    <div>
        <h2>客户信息</h2>
        @*获取数据值*@
        <p>姓名：@Model.CustomerName</p>
        <p>地址：@Model.CustomerAddress</p>
    </div>

</body>
</html>

